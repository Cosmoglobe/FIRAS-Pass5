!
!  FIRAS CMBR temperature map
!    project dataset record structure
!
record FIP_TCB FIP_TCB
!
! FIRAS pixel number (resolution 6)
scalar/long            PIXEL
! Ecliptic longitude, epoch 2000.0, in deg (0 -> 360)
scalar/float           ECLON
!Ecliptic latitude, epoch 2000.0, in deg (-90 -> +90)
scalar/float           ECLAT
!
!  CMBR Temperature in Pixel
!
! CMBR Temperture in Pixel, in K
scalar/float           TEMP
! Sigma of CMBR Temperture in Pixel, in K
scalar/float           TEMP_SIG
! CMBR Temperature Residual in Pixel, in K
scalar/float           RESID_TEMP
!
!  Ancillary Information
!
! Channel / MTM scan mode (length and speed)
scalar/text/length=4   CHANSCAN
! Number of IFGs in fitted Spectrum
scalar/float           NUM_IFGS
! Galactic longitude, in deg (0 -> 360)
scalar/float           GALON
! Galactic latitude, in deg (-90 -> +90)
scalar/float           GALAT
! Right ascension, in deg (0 -> 360)
scalar/float           RA
! Declination, in deg (-90 -> +90)
scalar/float           DEC
! Galactic latitude exclusion for dipole fit, 
! absolute value in deg (0 -> 90)        
scalar/float           GALATEXC
!
end_record

!
! Sigmas of the engineering analog values.
!
        union

          map
            array/float/dim=16      SIG_A_LO_GRT
                                    !  A side low current GRTS
            array/float/dim=16      SIG_A_HI_GRT
                                    !  A side high current GRTS
            array/float/dim=16      SIG_B_LO_GRT
                                    !  B side low current GRTS
            array/float/dim=16      SIG_B_HI_GRT
                                    !  B side high current GRTS
          endmap

          map
            !  A side low current GRTS
            scalar/float            SIG_A_LO_XCAL_TIP
                                    !  External calibrator tip
            scalar/float            SIG_A_LO_SKYHORN
                                    !  Sky horn
            scalar/float            SIG_A_LO_REFHORN
                                    !  Reference horn
            scalar/float            SIG_A_LO_ICAL
                                    !  Internal reference source
            scalar/float            SIG_A_LO_DIHEDRAL
                                    !  Dihedral
            array/float/dim=4       SIG_A_LO_BOL_ASSEM
                                    !  Bolometer assembly
            scalar/float            SIG_A_LO_MIRROR
                                    !  Mirror assembly
            array/float/dim=4       SIG_A_LO_CAL_RESISTORS
                                    !  Cal resistors
            scalar/float            SIG_A_LO_XCAL_CONE
                                    !  Additional xcal temp
            scalar/float            SIG_A_LO_COLLIMATOR
                                    !  Collimator
            !  A side high current GRTS
            scalar/float            SIG_A_HI_XCAL_TIP
                                    !  External calibrator tip
            scalar/float            SIG_A_HI_SKYHORN
                                    !  Sky horn
            scalar/float            SIG_A_HI_REFHORN
                                    !  Reference horn
            scalar/float            SIG_A_HI_ICAL
                                    !  Internal reference source
            scalar/float            SIG_A_HI_DIHEDRAL
                                    !  Dihedral
            array/float/dim=4       SIG_A_HI_BOL_ASSEM
                                    !  Bolometer assembly
            scalar/float            SIG_A_HI_MIRROR
                                    !  Mirror assembly
            array/float/dim=4       SIG_A_HI_CAL_RESISTORS
                                    !  Cal resistors
            scalar/float            SIG_A_HI_XCAL_CONE
                                    !  Additional xcal temp
            scalar/float            SIG_A_HI_COLLIMATOR
                                    !  Collimator
            !  B side low current GRTS
            scalar/float            SIG_B_LO_XCAL_TIP
                                    !  External calibrator tip
            scalar/float            SIG_B_LO_SKYHORN
                                    !  Sky horn
            scalar/float            SIG_B_LO_REFHORN
                                    !  Reference horn
            scalar/float            SIG_B_LO_ICAL
                                    !  Internal reference source
            scalar/float            SIG_B_LO_DIHEDRAL
                                    !  Dihedral
            array/float/dim=4       SIG_B_LO_BOL_ASSEM
                                    !  Bolometer assembly
            scalar/float            SIG_B_LO_MIRROR
                                    !  Mirror assembly
            array/float/dim=4       SIG_B_LO_CAL_RESISTORS
                                    !  Cal resistors
            scalar/float            SIG_B_LO_XCAL_CONE
                                    !  Additional xcal temp
            scalar/float            SIG_B_LO_COLLIMATOR
                                    !  Collimator
            !  B side high current GRTS
            scalar/float            SIG_B_HI_XCAL_TIP
                                    !  External calibrator tip
            scalar/float            SIG_B_HI_SKYHORN
                                    !  Sky horn
            scalar/float            SIG_B_HI_REFHORN
                                    !  Reference horn
            scalar/float            SIG_B_HI_ICAL
                                    !  Internal reference source
            scalar/float            SIG_B_HI_DIHEDRAL
                                    !  Dihedral
            array/float/dim=4       SIG_B_HI_BOL_ASSEM
                                    !  Bolometer assembly
            scalar/float            SIG_B_HI_MIRROR
                                    !  Mirror assembly
            array/float/dim=4       SIG_B_HI_CAL_RESISTORS
                                    !  Cal resistors
            scalar/float            SIG_B_HI_XCAL_CONE
                                    !  Additional xcal temp
            scalar/float            SIG_B_HI_COLLIMATOR
                                    !  Collimator
          endmap

          map
            array/float/dim=64      SIG_GRT
          endmap

        endunion

        union

          map

            array/float/dim=8       SIG_TEMP_CTRL
                                    !  Temperature controllers
            array/float/dim=2       SIG_IPDU_TEMP
                                    !  IPDU temps A,B
            array/float/dim=4       SIG_CNA_TEMP
                                    !  Channel temps
            array/float/dim=2       SIG_DBX_TEMP
                                    !  Drive box temps
            array/float/dim=2       SIG_STAT_MON_TEMP
                                    !  Status monitor temp
            scalar/float            SIG_PAMP_CHAN
                                    !  Channel preamp
            scalar/float            SIG_PAMP_OP
                                    !  Optical preamp
            array/float/dim=2       SIG_HOT_SPOT
                                    !  Hot spot heater A,B
            array/float/dim=2       SIG_MTM_CAL_MTR
                                    !  MTM/Cal motors
            array/float/dim=2       SIG_MTM_POS
                                    !  MTM position
            array/float/dim=4       SIG_BOL_VOLT
                                    !  Bolometer voltages
            array/float/dim=20      SIG_IPDU_VOLT
                                    !  IPDU voltages
            array/float/dim=12      SIG_IPDU_AMP
                                    !  IPDU currents
          endmap

          map
            array/float/dim=62     GROUP2
          endmap

        endunion

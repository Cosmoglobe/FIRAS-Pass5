!
! GRT Weights for Coadd Data
!
record FEX_GRTCOAWT FEX_GRTCOAWT

  structure CT_HEAD
    include ct_head.rdl
  endstructure

  scalar/long           NUM_GRT
                        ! Number of GRTs

  array/float/dim=16    GRT_A_WEIGHT
  array/float/dim=16    GRT_B_WEIGHT
                        ! Significance of GRT
                        ! Temperature

  array/byte/dim=60     GRT_SPARES
                        ! Pad to 256 bytes

end_record

!
! Instrument temperatures.
!
        union

          map

            array/float/dim=16      A_LO_GRT
                                    !  A side low current GRTS
            array/float/dim=16      A_HI_GRT
                                    !  A side high current GRTS
            array/float/dim=16      B_LO_GRT

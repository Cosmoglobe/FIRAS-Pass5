            scalar/float            A_LO_MIRROR
                                    !  Mirror assembly
            array/float/dim=4       A_LO_CAL_RESISTORS
                                    !  Cal resistors
            scalar/float            A_LO_XCAL_CONE
                                    !  Additional xcal temp
            scalar/float            A_LO_COLLIMATOR
                                    !  Collimator
            !  A side high current GRTS
            scalar/float            A_HI_XCAL_TIP

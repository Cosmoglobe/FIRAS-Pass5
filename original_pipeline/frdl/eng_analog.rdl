            scalar/float            A_LO_SKYHORN
                                    !  Sky horn
            scalar/float            A_LO_REFHORN
                                    !  Reference horn
            scalar/float            A_LO_ICAL
                                    !  Internal reference source
            scalar/float            A_LO_DIHEDRAL
                                    !  Dihedral
            array/float/dim=4       A_LO_BOL_ASSEM

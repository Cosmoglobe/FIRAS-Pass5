        union

          map

            array/float/dim=8       TEMP_CTRL
                                    !  Temperature controllers
            array/float/dim=2       IPDU_TEMP
                                    !  IPDU temps A,B
            array/float/dim=4       CNA_TEMP
                                    !  Channel temps
            array/float/dim=2       DBX_TEMP
                                    !  Drive box temps
            array/float/dim=2       STAT_MON_TEMP
                                    !  Status monitor temp
            scalar/float            PAMP_CHAN
                                    !  Channel preamp
            scalar/float            PAMP_OP
                                    !  Optical preamp
            array/float/dim=2       HOT_SPOT
                                    !  Hot spot heater A,B
            array/float/dim=2       MTM_CAL_MTR
                                    !  MTM/Cal motors
            array/float/dim=2       MTM_POS
                                    !  MTM position
            array/float/dim=4       BOL_VOLT
                                    !  Bolometer voltages
            array/float/dim=20      IPDU_VOLT
                                    !  IPDU voltages
            array/float/dim=12      IPDU_AMP
                                    !  IPDU currents
          endmap

          map
            array/float/dim=62     GROUP1
          endmap

        endunion

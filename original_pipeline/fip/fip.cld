!  Command Language Definition file for facility FIP_INITIAL_PRODUCT

!
!  FIP_SKY
!
	DEFINE VERB FIPSKY

		IMAGE "CSDR$SYSTEM:FIP_SKY.EXE"

			QUALIFIER FILE,			!  Input Filename
				VALUE(TYPE=$FILE,REQUIRED),
				NONNEGATABLE

			QUALIFIER EXC_GLAT,		!  Galactic Latitude Cut
				VALUE,
				NEGATABLE

			QUALIFIER FREQ_RANGE		!  Frequency Range Cut
				VALUE(TYPE=FIP_FREQ,LIST),
				DEFAULT,
				NONNEGATABLE

			QUALIFIER REPORT,		!  Processing Report
				VALUE(TYPE=$FILE),
				DEFAULT,
				NEGATABLE

			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.LOW
			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.HIGH
			DISALLOW FREQ_RANGE.LOW AND (NOT FREQ_RANGE.HIGH)
			DISALLOW FREQ_RANGE.HIGH AND (NOT FREQ_RANGE.LOW)


!
!  FIP_MODEL
!
	DEFINE VERB FIPMODEL

		IMAGE "CSDR$SYSTEM:FIP_MODEL.EXE"

			QUALIFIER MODEL_EXT,		!  Model File Extension
				VALUE(TYPE=$FILE,REQUIRED),
				NONNEGATABLE

			QUALIFIER CHANNEL,		!  Channel Specifier
				VALUE(TYPE=FIP_CHAN,REQUIRED),
				NONNEGATABLE

			QUALIFIER SCAN_MODE,		!  Scan Mode Specifier
				VALUE(TYPE=FIP_SCAN,REQUIRED)
				NONNEGATABLE

			QUALIFIER FLIGHT		!  Reference Time

			QUALIFIER INT			!  Reference Time

			QUALIFIER BIAS,			!  Commanded Bias
				VALUE

			QUALIFIER VOLT,			!  Bolometer Voltage
				VALUE

			QUALIFIER TEMP,			!  Bolometer Temperature
				VALUE

			QUALIFIER FREQ_RANGE		!  Frequency Range Cut
				VALUE(TYPE=FIP_FREQ,LIST),
				DEFAULT,
				NONNEGATABLE

			QUALIFIER REPORT,		!  Processing Report
				VALUE(TYPE=$FILE),
				DEFAULT,
				NEGATABLE

			DISALLOW FLIGHT AND INT
			DISALLOW (NOT FLIGHT) AND (NOT INT)
			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.LOW
			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.HIGH
			DISALLOW FREQ_RANGE.LOW AND (NOT FREQ_RANGE.HIGH)
			DISALLOW FREQ_RANGE.HIGH AND (NOT FREQ_RANGE.LOW)

!  FIP_COVAR
!
	DEFINE VERB FIPCOVAR

		IMAGE "CSDR$SYSTEM:FIP_COVAR.EXE"

			QUALIFIER FILE_EXT,
				VALUE (TYPE = $FILE, REQUIRED),
				NONNEGATABLE

			QUALIFIER CHANNEL,
				VALUE (TYPE = FIP_CHAN, REQUIRED),
				NONNEGATABLE

			QUALIFIER SCAN_MODE,
				VALUE (TYPE = FIP_SCAN, REQUIRED),
				NONNEGATABLE

			QUALIFIER FREQ_RANGE,
				VALUE (TYPE = FIP_FREQ, LIST, REQUIRED),
				NONNEGATABLE

			QUALIFIER REPORT,
				VALUE (TYPE = $FILE),
				DEFAULT,
				NEGATABLE

			DISALLOW FREQ_RANGE.LOW AND (NOT FREQ_RANGE.HIGH)
			DISALLOW FREQ_RANGE.HIGH AND (NOT FREQ_RANGE.LOW)



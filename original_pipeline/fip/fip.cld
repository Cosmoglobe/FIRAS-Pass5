!
!  FIP_SPECTRA_COADD
!
	DEFINE VERB FIPA

		IMAGE "CSDR$SYSTEM:FIP_SPECTRA_COADD.EXE"

			QUALIFIER SCRIPT,		!  Script Filename
				VALUE(TYPE=$FILE,REQUIRED),
				NONNEGATABLE

! SPECTRA = Calibrated, Differential, or Coadd

			QUALIFIER SPECTRA,
				VALUE (TYPE=FIP_SPECTRA,REQUIRED),
				NONNEGATABLE

			QUALIFIER DATA,			! Sky or Cal
				VALUE (TYPE=FIP_DATA,REQUIRED),
				NONNEGATABLE

			QUALIFIER CHANNEL,		! Channel to process.
				VALUE (TYPE=FIP_CHAN,REQUIRED),
				NONNEGATABLE

			QUALIFIER SCAN_MODE,		! Scan mode to process.
				VALUE (TYPE=FIP_SCAN,REQUIRED),
				NONNEGATABLE

			QUALIFIER EXC_GLAT,	!  Galactic Latitude Cut
				VALUE,
				NEGATABLE

			QUALIFIER FREQ_RANGE		!  Frequency Range Cut
				VALUE(TYPE=FIP_FREQ,LIST),
				DEFAULT,
				NONNEGATABLE

			QUALIFIER REPORT,		!  Processing Report
				VALUE(TYPE=$FILE),
				DEFAULT,
				NEGATABLE

			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.LOW
			DISALLOW FREQ_RANGE.ALL AND FREQ_RANGE.HIGH
			DISALLOW FREQ_RANGE.LOW AND (NOT FREQ_RANGE.HIGH)
			DISALLOW FREQ_RANGE.HIGH AND (NOT FREQ_RANGE.LOW)
!
!  Type Definitions
!

			DEFINE TYPE FIP_CHAN
				KEYWORD RH
				KEYWORD RL
				KEYWORD LH
				KEYWORD LL

			DEFINE TYPE FIP_SCAN
				KEYWORD SS
				KEYWORD SF
				KEYWORD LS
				KEYWORD LF

			DEFINE TYPE FIP_FEFSCAN
				KEYWORD RHSS
				KEYWORD RHFA
				KEYWORD RLSS
				KEYWORD RLFA
				KEYWORD RLLF
				KEYWORD LHSS
				KEYWORD LHFA
				KEYWORD LLSS
				KEYWORD LLFA
				KEYWORD LLLF
				KEYWORD HIGH
				KEYWORD LOWF
				KEYWORD LRES
				KEYWORD HRES

			DEFINE TYPE FIP_FINALSCAN
				KEYWORD HIGH
				KEYWORD LOWF
				KEYWORD LRES
				KEYWORD HRES

			DEFINE TYPE FIP_FREQ
				KEYWORD ALL, DEFAULT
				KEYWORD LOW,
					VALUE(TYPE=$NUMBER,REQUIRED)
				KEYWORD HIGH,
					VALUE(TYPE=$NUMBER,REQUIRED)

			DEFINE TYPE FIP_SPECTRA
				KEYWORD DIFFERENTIAL
				KEYWORD CALIBRATED
				KEYWORD COADD

			DEFINE TYPE FIP_DATA
				KEYWORD SKY
				KEYWORD CAL
